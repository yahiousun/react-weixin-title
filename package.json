{
  "name": "react-weixin-title",
  "version": "1.0.2",
  "description": "Provides a declarative way to specify document.title in weixin for iOS.",
  "repository": "yahiousun/react-weixin-title",
  "main": "scr/index.js",
  "babel": {
    "presets": [
      "latest",
      "stage-0",
      "react"
    ],
    "plugins": [
      "transform-runtime"
    ]
  },
  "scripts": {
    "build": "npm run build:lib && npm run build:example",
    "build:lib": "node tools/build",
    "build:example": "del ./dist/example && webpack --config webpack.config.example.js --progress --colors -p",
    "lint": "eslint ./src ./example ./test",
    "start": "webpack-dev-server --config webpack.config.dev.js --hot --inline --progress --colors --port 8080",
    "test": "karma start"
  },
  "author": "yahiousun <yahiousun@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "babel-runtime": "^6.11.6",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-router": "^3.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.5.3",
    "babel": "^6.5.2",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.8",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.18.0",
    "chai": "^3.5.0",
    "css-loader": "^0.26.0",
    "del": "^2.2.2",
    "del-cli": "^0.2.0",
    "enzyme": "^2.6.0",
    "eslint": "^3.11.0",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.7.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.24.1",
    "isparta-loader": "^2.0.0",
    "istanbul": "^0.4.5",
    "json-loader": "^0.5.4",
    "karma": "^1.3.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.1",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.8.0",
    "mocha": "^3.2.0",
    "node-sass": "^3.13.0",
    "normalize.css": "^5.0.0",
    "open-browser-webpack-plugin": "^0.0.3",
    "postcss-loader": "^1.1.1",
    "react-addons-css-transition-group": "^15.4.1",
    "react-addons-test-utils": "^15.4.1",
    "react-syntax-highlighter": "^3.0.0",
    "rollup": "^0.36.4",
    "rollup-plugin-babel": "^2.6.1",
    "sass-loader": "^4.0.2",
